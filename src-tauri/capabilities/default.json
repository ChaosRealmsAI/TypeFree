{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "POC permissions",
  "windows": ["main", "stt", "overlay", "doubao-asr"],
  "remote": {
    "urls": ["https://*.doubao.com/*"]
  },
  "permissions": [
    "core:default",
    "core:event:default",
    {
      "identifier": "core:event:allow-emit",
      "allow": [{ "event": "*" }]
    },
    {
      "identifier": "core:event:allow-listen",
      "allow": [{ "event": "*" }]
    }
  ]
}
